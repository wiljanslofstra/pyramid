    "webpack-prod": "NODE_ENV=production webpack --watch --env=production --config=build/webpack.config.js",
    "test": "NODE_ENV=test npm run test-runner",
    "test-runner": "node ./node_modules/karma/bin/karma start build/karma.conf",
    "sass": "nodemon --exec \"node bin/sass\" src/styles/*",
    "codecov": "cat coverage/*/lcov.info | codecov",
